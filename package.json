		"dotenv-cli": "^8.0.0",
		"drizzle-kit": "^0.30.4",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.5",
		"tsup": "^8.3.6"
	},
	"dependencies": {
